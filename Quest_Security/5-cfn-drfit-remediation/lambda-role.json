{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": "*"
        },
{
            "Action": [
                "cloudformation:Describe*",
                "cloudformation:Detect*",
                "cloudformation:Get*",
                "cloudformation:List*",
                "cloudformation:TagResource",
                "cloudformation:UntagResource",
                "cloudformation:ValidateTemplate",
                "ec2:AssociateIamInstanceProfile",
                "ec2:AuthorizeSecurityGroupEgress",
                "ec2:AuthorizeSecurityGroupIngress",
                "ec2:CreateSecurityGroup",
                "ec2:DeleteSecurityGroup",
                "ec2:Describe*",
                "ec2:DisassociateIamInstanceProfile",
                "ec2:Get*",
                "ec2:List*",
                "ec2:ModifySecurityGroupRules",
                "ec2:RevokeSecurityGroupEgress",
                "ec2:RevokeSecurityGroupIngress",
                "ec2:UpdateSecurityGroupRuleDescriptionsEgress",
                "ec2:UpdateSecurityGroupRuleDescriptionsIngress",
                "iam:ListInstanceProfiles",
                "s3:Get*",
                "s3:List*",
                "s3:Put*"
            ],
            "Resource": "*",
            "Effect": "Allow"
        },
        {
            "Condition": {
                "StringEquals": {
                    "aws:RequestedRegion": "us-east-1"
                }
            },
            "Action": "lambda:InvokeFunction",
            "Resource": "*",
            "Effect": "Allow"
        },
        {
            "Action": [
                "iam:ListInstanceProfiles",
                "iam:PassRole"
            ],
            "Resource": [
                "arn:aws:iam::446502295847:instance-profile/Sample_Instance_Profile_A",
                "arn:aws:iam::446502295847:instance-profile/Sample_Instance_Profile_B",
                "arn:aws:iam::446502295847:role/ec2_sample-role_a",
                "arn:aws:iam::446502295847:role/ec2_sample-role_b"
            ],
            "Effect": "Allow"
        },
        {
            "Action": "*",
            "Resource": [
                "arn:aws:lambda:us-east-1:446502295847:function:gbl_lab_monitoring",
                "arn:aws:iam::446502295847:role/LabStack-c1a3e936-0ffc-40-GblLabMonitoringgbllabmon-hWNjmP2V8cE5",
                "arn:aws:events:us-east-1:446502295847:rule/LabStack-c1a3e936-0ffc-40-GblLabMonitoringmonitorin-vGxdClH7uz5e",
                "arn:aws:events:us-east-1:446502295847:rule/LabStack-c1a3e936-0ffc-40-GblLabMonitoringmonitorin-lgpfIisxsh9d",
                "arn:aws:events:us-east-1:446502295847:rule/LabStack-c1a3e936-0ffc-40-GblLabMonitoringmonitorin-o9f4rJbT2SXB",
                "arn:aws:events:us-east-1:446502295847:rule/LabStack-c1a3e936-0ffc-40-GblLabMonitoringmonitorin-nBEAlnMnB76e",
                "arn:aws:events:us-east-1:446502295847:rule/LabStack-c1a3e936-0ffc-40-GblLabMonitoringmonitorin-WLyAYARD0Xc2",
                "arn:aws:events:us-east-1:446502295847:rule/LabStack-c1a3e936-0ffc-40-GblLabMonitoringmonitorin-7UFLWUfguNBp"
            ],
            "Effect": "Deny"
        },
        {
            "Action": "lambda:PutProvisionedConcurrencyConfig",
            "Resource": "*",
            "Effect": "Deny"
        }        
    ]
}
